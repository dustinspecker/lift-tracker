{
  "env": {
    "development": {
      "plugins": [
        "react-display-name",
        ["react-transform", {
          "transforms": [
            {
              "transform": "react-transform-render-visualizer"
            }
          ]
        }]
      ]
    }
  },
  "plugins": [
    "syntax-object-rest-spread",
    "transform-object-rest-spread"
  ],
  "presets": [
    "es2015",
    "react"
  ]
}
